{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Run Cleaner Script",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/scripts/CleanTemporaryFiles.ps1",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "PowerShell: Register Shortcuts",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/scripts/RegisterShortcuts.ps1",
            "cwd": "${workspaceFolder}"
        }
    ]
}